<div class="workgroup" data-id="{{ workgroup.id }}" data-slug="{{ workgroup.slug }}">
	<div class="header">
		<form id="workgroup-form-edit-{{ workgroup.id }}" class="form" action="{{ path('BtaskBoardBundle_workgroup_update', { 'id': workgroup.id }) }}" method="post" {{ form_enctype(form) }}>
			{{ form_errors(form) }}

    		{{ form_errors(form.name) }}
    		{{ form_widget(form.name) }}
			{{ form_rest(form) }}
			<input class="btn" type="reset" value="{{ 'navigation.workgroup.reset'|trans }}" />
			<input class="btn" type="submit" value="{{ 'navigation.workgroup.update'|trans }}" />
		</form>
	</div>
</div>
